[node jf_nested_dielectric]
    desc                    STRING      "Nested Dielectric Shader"
    maya.name               STRING      "jf_nested_dielectric"
    maya.classification     STRING      "shader/surface"
    maya.id                 INT         0x67993104
    
        
    # AOVs
    [attr aov_direct_refraction]
        aov.type                INT     0x05
        default STRING ""
    [attr aov_indirect_refraction]
        aov.type                INT     0x05
        default STRING "refraction"
    [attr aov_direct_specular]
        aov.type                INT     0x05
        default STRING "direct_specular"
    [attr aov_indirect_specular]
        aov.type                INT     0x05
        default STRING "indirect_specular"



